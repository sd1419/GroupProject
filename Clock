import time
import random

start_time = time.time()
HR_list = []

while True:
    current_time = time.time()
    elapsed_time = current_time - start_time
    if elapsed_time >= 100:
        break
    HR = random.randint(50, 120)
    HR_list.append(HR)

    database = open("HR_database.txt", "w")
    database.write(str(HR) + "\n")
    database.close

    print(HR_list)
    time.sleep(5)

    